<!--pages/index/team/team.wxml-->
<view class='weui-cells' style='margin-top:0'>
  <view class='weui-cell' bindtap='listTeam' data-type="under">
    <view class='weui-cell__hd'>
      <image src='../../../assets/images/icon_tabbar.png' class='team_icon'></image>
    </view>
    <view class='weui-cell__bd'>
      <view class='base_title'>直属团队</view>
      <view class='base_title'>{{under_total}}人</view>
    </view>
    <view class='weui-cell__ft weui-cell__ft_in-access'></view>
  </view>
  <view class='weui-cell' bindtap='listTeam' data-type="Belong">
    <view class='weui-cell__hd'>
      <image src='../../../assets/images/icon_tabbar.png' class='team_icon'></image>
    </view>
    <view class='weui-cell__bd'>
      <view class='base_title'>间接团队</view>
      <view class='base_title'>{{Belong_total}}人</view>
    </view>
    <view class='weui-cell__ft weui-cell__ft_in-access'></view>
  </view>
</view>
